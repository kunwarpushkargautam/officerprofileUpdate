<script>
    var warningtextmessage = document.getElementById("warningtextmessage");
    var warningMessage = document.getElementById("warningMessage");
    
    
    console.log(warningtextmessage,"direct")
    if(warningtextmessage.innerText){
        {{!-- warningtextmessage.insertAdjacentHTML('beforeend', '<span><i class="fa fa-exclamation-triangle mx-2" aria-hidden="true">   </i></span>'); --}}
        warningMessage.style.display="block"

    }else{
        warningMessage.style.display="none"
    }
</script>
<script>
    var helpNoticeText = document.getElementById("helpNoticeText");
    var helpnotice = document.getElementById("helpnotice");
    
    console.log(helpNoticeText,"direct")
    if(helpNoticeText.innerText && helpNoticeText != ""){
        helpNoticeText.insertAdjacentHTML('beforeend', '<span><i class="fa fa-exclamation-triangle mx-2" aria-hidden="true">   </i></span>');
        helpnotice.style.display="block";

    }else{
        helpnotice.style.display="none";
    }
</script>

